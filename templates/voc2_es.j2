<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
{% set language = "es" %}
{% set sortattr = "sort" %}

<html xml:lang="en" lang="en" dir="ltr" typeof="bibo:Document " about="" property="dcterms:language" content="en" prefix="dcterms: http://purl.org/dc/terms/ bibo: http://purl.org/ontology/bibo/ schema: http://schema.org/ w3p: http://www.w3.org/2001/02pd/rec54#" xmlns="http://www.w3.org/1999/xhtml">
  
  <head>
    <title>{{ metadata.title }}</title>
    <meta charset='utf-8' />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="https://dij151upo6vad.cloudfront.net/2.latest/css/vlaanderen-ui.css" />
    <link rel="stylesheet" href="https://dij151upo6vad.cloudfront.net/2.latest/css/vlaanderen-ui-corporate.css" />
    <link rel="stylesheet" href="https://dev.w3.org/2009/dap/ReSpec.js/css/respec.css" />

    <link rel="icon" sizes="192x192" href="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/icon-highres-precomposed.png" />
    <link rel="apple-touch-icon" href="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/touch-icon-iphone-precomposed.png" />
    <link rel="apple-touch-icon" sizes="76x76" href="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/touch-icon-ipad-precomposed.png" />
    <link rel="apple-touch-icon" sizes="120x120" href="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/touch-icon-iphone-retina-precomposed.png" />
    <link rel="apple-touch-icon" sizes="152x152" href="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/touch-icon-ipad-retina-precomposed.png" />

    <meta name="msapplication-square70x70logo" content="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/tile-small.png" />
    <meta name="msapplication-square150x150logo" content="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/tile-medium.png" />
    <meta name="msapplication-wide310x150logo" content="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/tile-wide.png" />
    <meta name="msapplication-square310x310logo" content="https://dij151upo6vad.cloudfront.net/2.latest/icons/app-icon/tile-large.png" />
    <meta name="msapplication-TileColor" content="#FFE615" />

    <!--[if lt IE 9]><script src='https://www.w3.org/2008/site/js/html5shiv.js'></script><![endif]-->
  </head>
  <body class="h-entry" role="document" id="respecDocument">
    <div style="min-height:44px;;" id="vlaanderen-header">
    <!-- insert your global header here -->
    <script type="text/javascript" src="https://widgets.vlaanderen.be/widget/live/5ecf33a0a922428cb9d944c55362ed15"></script>
    <!-- end global header-->
    </div>
    <div class="page">
        <div class="layout layout--wide">
            <div class="grid u-hr">
                <div class="col--9-12 col--1-1--s">
                    <div class="head" role="contentinfo" id="respecHeader">
                    <h1 class="h1 title p-name"  property="dcterms:title" id="title">{{ metadata.title }}</h1>

                    {% if metadata.prefix %}
                    <h2 class="h2" id="subtitle" property="bibo:subtitle">Specification for <em>{{ metadata.prefix }}</em></h2>
                    {% endif %}

              <div class="head" role="status" id="status">
              <dl>
              <dt>Estatus</dt><dd><a href="{{metadata.status}}">{{metadata.status}}</a></dd>
              <dt>Fecha de publicación</dt><dd>{{metadata.issued}}</dd>
              <dt>Versión actual</dt><dd><a href="{{metadata.navigation.self}}">{{metadata.navigation.self}}</a></dd>
              {% if metadata.navigation.next %}
              <dt>Próxima versión</dt><dd><a href="{{metadata.navigation.next}}">{{metadata.navigation.next}}</a></dd>
              {% endif %}
              {% if metadata.navigation.prev %}
              <dt>Versión previa</dt><dd><a href="{{metadata.navigation.prev}}">{{metadata.navigation.prev}}</a></dd>
              {% endif %}
              </dl>
              </div>


                        {% for role, contributors in contributors | groupby("role") %}
                          {% if role == "A" %}
                            {% set authors = contributors|sort(attribute='last_name') %}
                        {% if authors %}
                          <dt>Autores/autoras</dt>
                          {% for author in authors %}
                          <dd inlist="" rel="foaf:maker" class="p-author h-card vcard">
                              <span typeof="foaf:Person">
                              <span class="p-name fn" property="foaf:lastName">{{ author.last_name }}</span>,
                              <span class="p-name fn" property="foaf:firstName">{{ author.first_name }}</span>
                              {% if author.affiliation.website and author.affiliation.name %}
                              - <a href="{{ author.affiliation.website }}" class="p-org org h-org h-card" rel="foaf:workplaceHomepage">{{ author.affiliation.name }}</a>
                              {% elif author.affiliation.name %}
                              - <span class="p-org org h-org h-card">{{ author.affiliation.name }}</span>
                              {% endif %}
                							{% if author.email %}
                               - <span class="ed_mailto"><a href="mailto:{{ author.email }}" rel="foaf:mbox" class="u-email email">{{ author.email }}</a></span>
                              {% endif %}
                              </span>
                          </dd>
                          {% endfor %}
                        {% endif %}
                      {% elif role == "E" %}
                        {% set editors = contributors|sort(attribute='last_name') %}
                        {% if editors %}
                          <dt>Editores/editoras</dt>
                          {% for editor in editors %}
                          <dd inlist="" rel="bibo:editor" class="p-author h-card vcard">
                              <span typeof="foaf:Person">
                              <span class="p-name fn" property="foaf:lastName">{{ editor.last_name }}</span>,
                              <span class="p-name fn" property="foaf:firstName">{{ editor.first_name }}</span>
                              {% if editor.affiliation.website and editor.affiliation.name %}
                                 - <a href="{{ editor.affiliation.website }}" class="p-org org h-org h-card" rel="foaf:workplaceHomepage">{{ editor.affiliation.name }}</a>
                              {% elif editor.affiliation.name %}
                                 - <span class="p-org org h-org h-card">{{ editor.affiliation.name }}</span>
                              {% endif %}
                							{% if editor.email %}
                                 - <span class="ed_mailto"><a href="mailto:{{ editor.email }}" rel="foaf:mbox" class="u-email email">{{ editor.email }}</a></span>
                              {% endif %}
                							</span>
                          </dd>
                        {% endfor %}
                        {% endif %}
                      {% elif role == "C" %}
                        {% set pure_contributors = contributors|sort(attribute='last_name') %}
                        {% if pure_contributors %}
                        <dt>Contribuyentes</dt>
                          {% for contributor in pure_contributors %}
                          <dd inlist="" rel="dcterms:contributor" class="p-author h-card vcard">
                              <span typeof="foaf:Person">
                              <span class="p-name fn" property="foaf:lastName">{{ contributor.last_name }}</span>,
                              <span class="p-name fn" property="foaf:firstName">{{ contributor.first_name }}</span>
                              {% if contributor.affiliation.website and contributor.affiliation.name %}
                              - <a href="{{ contributor.affiliation.website }}" class="p-org org h-org h-card" rel="foaf:workplaceHomepage">{{ contributor.affiliation.name }}</a>
                              {% elif contributor.affiliation.name %}
                              - <span class="p-org org h-org h-card">{{ contributor.affiliation.name }}</span>
                              {% endif %}
                							{% if contributor.email %}
                               - <span class="ed_mailto"><a href="mailto:{{ contributor.email }}" rel="foaf:mbox" class="u-email email">{{ contributor.email }}</a></span>
                              {% endif %}
							                </span>
                            </dd>
                            {% endfor %}
                          {% endif %}
                        {% endif %}
                      {% endfor %}
                    </dl>
                    </div>
                </div>
              <div class="head" role="metainfo" id="metainfo">
              <dl>
              <dt>Origen</dt><dd><a href="{{metadata.repositoryurl}}">{{metadata.repositoryurl}}</a></dd>
              <dt>Registro de cambios</dt><dd><a href="{{metadata.changelogurl}}">{{metadata.changelogurl}}</a></dd>
              <dt id="feedbackurl">Comentarios y retroalimentación</dt><dd><a href="{{metadata.feedbackurl}}">{{metadata.feedbackurl}}</a></dd>
              <dt>Registro de estándares</dt><dd><a href="{{metadata.standaardregisterurl}}">{{metadata.standaardregisterurl}}</a></dd>

              <dt>Dependencias</dt><dd>
                {% for dep in metadata.dependencies %}
		<a href="{{ dep.packageurl }}">{{ dep.packagelabel }}</a>
                {% endfor %}
		</dd>
              </dl>
              </div>
             </div>
                <!-- <p class="copyright">{{ metadata.rights }}</p> -->
        </div>
    </div>
    <div id="main" itemprop="mainContentOfPage" role="main" tabindex="-1" class="main-content">
        <div class="region region--no-space-top">
            <div class="layout layout--wide">
                <div class="grid">
                    <div class="col--8-12 col--12-12--s">
            <div class="region" id="introduction">
              <div class="introduction typography">
              {% block introduction %}
                <p>Este documento describe un <strong>vocabularium</strong>, para este caso <strong>{{ metadata.title }}</strong>.
                   Un vocabulario es una colección de términos reutilizables (clases y propiedades), cuyo significado viene dado por una etiqueta y una definición.
                   Hay acuerdos formales adicionales disponibles para los términos definidos en este dominio.
                   Los acuerdos formales adicionales se pueden encontrar en fuentes externas para términos externos.
                </p>
              {% endblock %}
              </div>
            </div>
                        <div class="region region">
                            <section id="abstract" class="introductory" property="dcterms:abstract" datatype="" typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter">
                                <h2 class="h2" aria-level="1" role="heading" id="h2_abstract">Resumen</h2>
                                <div class="region region--no-space-top typography" id="summary"> 
				{% block summary %}
                                <p>
				El vocabulario {{ metadata.title }} especifica clases y propiedades para describir el dominio {{ metadata.title | más bajo }}.
                                </p>
                                <p>
                                {{ metadata.abstract[language] }}
                                </p>
                                {% endblock %}
				</div>
                            </section>
                        </div>

                        <div class="region region--no-space-top">
                            <section class="introductory" id="sotd" typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter">
                                <h2 class="h2" aria-level="1" role="heading" id="h2_sotd">Estado de este documento</h2>
                                <div class="region region--no-space-top typography" id="status"> 
				{% block status %}
                                <p><em>Esta sección describe el estado de este documento en el momento de su publicación. Es posible que, mientras tanto, se hayan lanzado versiones más nuevas.</em></p><br/>
                                 <p>Puede encontrar una lista del vocabulario más reciente aquí: <a href="http://data.vlaanderen.be/ns">http://data.vlaanderen.be/ns</a></ pag><br/>
                                 <p>Este documento es una especificación oficial de un vocabulario y proporciona un contexto compartido de términos para algunos conceptos. Esta especificación sirve para aclarar la definición, el uso, el dominio y el rango de los términos RDF dentro de este dominio. Los términos definidos en este vocabulario son persistentes y, como consecuencia, no cambiarán excepto para aclarar aún más la definición existente. Los términos pueden marcarse como obsoletos y reemplazarse en versiones más recientes de esta especificación.</p>
                                 {% if metadata.comment[language] %}
                                <p>
                                {{ metadata.comment[language] }}
                                </p>
                                {% endif %}
                                {% endblock %}
				</div>
                            </section>
                        </div>

                        <div class="region region--no-space-top">
                            <section typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter" id="license-and-liability">
                                <h2 class="h2" id="licenseandliability" aria-level="1" role="heading"><span class="secno">1. </span>Licencia</h2>
                                {% block license %}
              <p><span rel="dct:type" property="dct:title" href="http://purl.org/dc/dcmitype/Text" xmlns:dct="http://purl.org/dc/terms/">Esta especificación</span> por <a rel="cc:attributionURL" property="cc:attributionName" href="http://vlaanderen.be/informatievlaanderen" xmlns:cc="http://creativecommons.org/ns #">Informatie Vlaanderen</a> se ha publicado bajo la siguiente licencia: <a href="{{ metadata.license }}" rel="license">Licencia de reutilización gratuita v1.0 ("Modellicentie Gratis Hergebruik - v1. 0")</a>.</p>
                                {% endblock %}
                            </section>
                        </div>

                        <div class="region region--no-space-top">
                            <section typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter" id="conformance-statement">
                                <h2 class="h2" id="conformance" aria-level="1" role="heading"><span class="secno">2. </span>Conformidad</h2>
                                {% block conformance %}
                                <p>Un intercambio de datos, cualquiera que sea la forma en que se produzca, se ajusta a este vocabulario mediante el uso de la terminología (clases y propiedades) de manera coherente con la semántica tal como se describe en la última versión de la especificación (dominio, rango, definición y uso) y no reemplaza términos en este vocabulario con términos alternativos de otros vocabularios que cubren el mismo contexto..</p>
                                {% endblock %}
                            </section>
                        </div>

                        <div class="region region--no-space-top">
                            <section typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter" id="overview">

                                <h2 class="h2" id="overview" aria-level="1" role="heading"><span class="secno">3. </span>Visión general</h2>

                                <div class="region region--no-space-top">
                                    <p>Esta sección enumera todas las clases y propiedades de este vocabulario.</p>
                                    <p>
                                    {% if externalvocs %}
                                    Este vocabulario utiliza términos del siguiente vocabulario:
                                    <ul>
                                    {% for term in externalvocs |sort(attribute=sortattr) %}
                                        <li><a href="{{ term.uri }}">{{ term.uri }}</a></li>
                                    {% endfor %}
                                    </ul>
                                    {% endif %}
                                    </p>
                                    <br />
                                </div>

                                {% if ( ( classes | length) > 0 )%}
                                <div class="region region--no-space-top">
                                    <h3 class="h3" >Clases</h3>
                                    <div class="grid">
                                        <p>
                                            |
                                            {% for class in classes | sort(attribute=sortattr) %}
                                            <a href="{{ class.uri  | replace(metadata.uri, '') }}" rel="class">{{ class.label[language] }}</a> |
                                            {% endfor %}
                                        </p>
                                    </div>
                                </div>
                                {% endif %}

                                {% if ( ( properties | length ) > 0 ) %}
                                <div class="region region--no-space-top">
                                    <h3 class="h3" >Propiedades</h3>
                                    <div class="grid">
                                        <p>
                                            |
                                            {% for prop in properties | sort(attribute=sortattr) %}
                                            <a href="{{ prop.uri  | replace(metadata.uri, '')}}" rel="property">{{ prop.label[language] }}</a> |
                                            {% endfor %}
                                        </p>
                                    </div>
                                </div>
                                {% endif %}
                            </section>
                        </div>

                        <div class="region region--no-space-top">
                            <section typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter" id="classes">

                                <h2 class="h2" id="classes" aria-level="1" role="heading"><span class="secno">4. </span>Clases</h2>
                                {% if classes %}
                                <div class="region region--no-space-top">
                                    <p>Esta sección define cada clase de manera formal.</p>
                                </div>
                                {% for class in classes | sort(attribute=sortattr) %}
                                <div class="region region--no-space-top">
                                    <h3 class="h3"  id="{{ class.uri | replace(metadata.uri + '#', '') }}">Clase <em>{{ class.label[language] }}</em></h3>
                                    <table class="definition">
                                        <tbody>
                                          <tr>
                                            <th>Type</th>
                                            <td>Clase</td>
                                          </tr>
<!-- The Naam of the property/class is not relevant as it is already in the title of the table
                                          {% if class.name %}
                                          <tr>
                                            <th>Nombre</th>
                                            <td><code>{{ class.label[language] }}</code></td>
                                          </tr>
                                          {% endif %}
-->
                                          <tr>
                                            <th>URI</th>
                                            <td><code>{{ class.uri }}</code></td>
                                          </tr>
                                          {% if ( ( class.parents | length ) > 0)  %}
										  <tr>
                                            <th>Definición más estrecha para</th>
											<td><ul>
					      {% for parent in class.parents|sort %}
                                              <li><code>{{ parent }}</code></li>
											{% endfor %}
                                            </ul></td>
										  </tr>
                                          {% endif %}
                                          <tr>
                                            <th>Definición</th>
                                            <td>{{ class.description[language]}}</td>
                                          </tr>
                                          {% if class.usage[language] %}
                                          <tr>
                                            <th>
                                              <p>Usage</p>
                                            </th>
                                            <td>{{ class.usage[language] }}</td>
                                          </tr>
                                          {% endif %}
                                        </tbody>
                                    </table>
                                </div>
                                {% endfor %}
                                {% else %}
                                <div class="region region--no-space-top">
                                    <p>No hay clases definidas para este vocabulario. Es posible que reutilice clases de vocabularios relacionados.</p>
                                </div>
                                {% endif %}
                            </section>
                        </div>


                        <div class="region region--no-space-top">
                            <section typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter" id="properties">

                                <h2 class="h2" id="properties" aria-level="1" role="heading"><span class="secno">5. </span>Propiedades</h2>

                                {% if properties %}
                                <div class="region region--no-space-top">
                                    <p>Esta sección define formalmente cada propiedad.</p>
                                </div>

                                {% for property in properties | sort(attribute=sortattr) %}
                                <div class="region region--no-space-top">
                                    <h3 class="h3"  id="{{ property.uri | replace(metadata.uri + '#', '') }}">Propiedad <em>{{  property.label[language] }}</em></h3>
                                    <table class="definition">
                                        <tbody>
                                          <tr>
                                            <th>Type</th>
                                            <td>Propiedad</td>
                                          </tr>
                                          <tr>
                                            <th>URI</th>
                                            <td><code>{{ property.uri }}</code></td>
                                          </tr>
                                          {% if ( ( property.domain  | length ) > 0 )%} 
                                          <tr>
                                            <th>Dominio</th>
                                            <td><code>
                                                {% for entity in property.domain %}
                                                  {{ entity }} 
                                                {% endfor %}
                                            </code></td>
                                          </tr>
                                          {% endif %} 
                                          {% if (( property.range  | length ) > 0)%}
                                          <tr>
                                            <th>Rango</th>
                                            <td><code>
                                                {% for entity in property.range %}
                                                  {{ entity }} 
                                                {% endfor %}
                                            </code></td>
                                          </tr>
                                          {% endif %}
                                          {% if ( ( property.parents | length ) > 0 )  %}
										                      <tr>
                                            <th>Más estrecho de</th>
											                      <td><ul>
											                        {% for parent in property.parents|sort %}
                                              <li><code>{{ parent }}</code></li>
                        											{% endfor %}
                                            </ul></td>
										                      </tr>
                                          {% endif %}
                                          <tr>
                                            <th>Definición</th>
                                            <td>{{ property.description[language]}}</td>
                                          </tr>
                                          {% if property.usage[language] %}
                                          <tr>
                                            <th>Uso</th>
                                            <td>{{ property.usage[language] }}</td>
                                          </tr>
                                          {% endif %}
                                        </tbody>
                                    </table>
                                </div>
                                {% endfor %}
                                {% else %}
                                <div class="region region--no-space-top">
                                    <p>No hay propiedades definidas en este vocabulario. Es posible que este vocabulario utilice propiedades definidas por el vocabulario relacionado.</p>
                                </div>
                                {% endif %}
                            </section>
                        </div>
                        <div class="region region--no-space-top">
                            <section typeof="bibo:Chapter" resource="#ref" rel="bibo:Chapter" id="external">

                                <h2 class="h2" id="externals" aria-level="1" role="heading"><span class="secno">6. </span>Terminología externa</h2>

                        
                                <div class="region region--no-space-top">
                                    <p>Esta sección enumera los términos definidos por otros vocabularios que tienen relevancia para este modelo de dominio con sus etiquetas y definiciones.</p>
                                </div>

                                {% if external_terms %}
                                {% for external in external_terms |sort(attribute=sortattr) %}
                                <div class="region region--no-space-top">
                                    <h3 class="h4" id="{{ external.name }}"><em>{{ external.label[language] }}</em></h3>
                                    <table class="definition">
                                        <tbody>
                                          
                                          <tr>
                                            <th>URI</th>
                                            <td><code>{{ external.uri }}</code></td>
                                          </tr>
					 {% if external.definition[language] %}
                                          <tr>
                                            <th>Definición</th>
                                            <td>{{ external.description[language] }}</td>
                                          </tr>
                                          {% endif %}
                                          {% if external.usage[language] %}
                                          <tr>
                                            <th>Usage</th>
                                            <td>{{ external.usage[language] }}</td>
                                          </tr>
                                          {% endif %}
                                          
                                        </tbody>
                                    </table>
                                </div>
                                {% endfor %}
                                {% else %}
                                <div class="region region--no-space-top">
                                    <p>No se utilizan términos externos en este vocabulario.</p>
                                </div>
                                {% endif %}

                            </section>
                        </div>
                    </div>
                    <div class="col--3-12 col--12-12--s col--4-12--m push--1-12 push--reset--m" id="sideNav">
                        <nav class="side-navigation js-sticky" id="sideNav">
                            <div class="side-navigation__content js-scrollspy">
                                <div class="side-navigation__group">
                                    <ul>
                                        <li class="side-navigation__item">
                                            <a href="#abstract">Resumen</a>
                                        </li>                                        
                                        <li class="side-navigation__item">
                                            <a href="#sotd">Estado de este documento</a>
                                        </li>                                        
                                        <li class="side-navigation__item">
                                            <a href="#license-and-liability">1. Licencia</a>
                                        </li>                                       
                                        <li class="side-navigation__item">
                                            <a href="#conformance-statement">2. Conformidad</a>
                                        </li>                                       
                                        <li class="side-navigation__item">
                                            <a href="#overview">3. Visión general</a>
                                        </li>                                    
                                        <li class="side-navigation__item">
                                            <a href="#classes">4. Clases</a>
                                        </li>                                    
                                        <li class="side-navigation__item">
                                            <a href="#properties">5. Properiedades</a>
                                        </li>                                  
                                        <li class="side-navigation__item side-navigation__item--parent">
                                            <a href="#external" data-child="external">6. Terminología externa</a>                                       
                                        </li>
                                    </ul>
                                </div>                                
                            </div>
                        </nav>
                    </div>                    
                </div>
            </div>
        </div>
    </div>

    <div id="vlaanderen-footer">
        <!-- insert your Vlaanderen-footer-code here -->
        <script type="text/javascript" src="https://widgets.vlaanderen.be/widget/live/d6d44414e7c242f396ab6da8f8da5198"></script>
        <!-- End global footer-->
    </div>

    <script src="https://dij151upo6vad.cloudfront.net/2.latest/js/vlaanderen-ui.js"></script>

    <style>
      @media screen {  /* hide from IE3 */
        a[href]:hover { background: #ffa }
      }
      table {
            font-size:12px;
            color:#333333;
            border-width: 1px;
            border-color: #666666;
            border-collapse: collapse;
            margin-top: 2em;
            width: 100%;
      }
      table th {
            border-width: 1px;
            padding: 8px;
            border-style: solid;
            border-color: #666666;
            background-color: #dedede;
            width: 110px;
      }
